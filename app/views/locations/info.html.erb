<div class="container" id="big">
<h1><%="Wiki on "+ session[:location].city+", "+ session[:location].state+", " +session[:location].country%> </h1>

<div class="contents">
<% session[:location].get_info.each  do |info| %>

<h4><%= link_to (info['title']), "http://en.wikipedia.org/wiki/"+info['title']%></h4>
<p><%= info['snippet'].html_safe %></p>

<% end %>
</div>
</div>


